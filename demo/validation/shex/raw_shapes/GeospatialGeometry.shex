<#ValidSchemaGeospatialGeometry>  EXTRA a {
	a [schema:GeospatialGeometry] * ;
	schema:geoContains (@<#ValidSchemaGeospatialGeometry> OR @<#ValidSchemaPlace>) * ;
	schema:geoCoveredBy (@<#ValidSchemaPlace> OR @<#ValidSchemaGeospatialGeometry>) * ;
	schema:geoCovers (@<#ValidSchemaGeospatialGeometry> OR @<#ValidSchemaPlace>) * ;
	schema:geoCrosses (@<#ValidSchemaGeospatialGeometry> OR @<#ValidSchemaPlace>) * ;
	schema:geoDisjoint (@<#ValidSchemaPlace> OR @<#ValidSchemaGeospatialGeometry>) * ;
	schema:geoEquals (@<#ValidSchemaPlace> OR @<#ValidSchemaGeospatialGeometry>) * ;
	schema:geoIntersects (@<#ValidSchemaGeospatialGeometry> OR @<#ValidSchemaPlace>) * ;
	schema:geoOverlaps (@<#ValidSchemaGeospatialGeometry> OR @<#ValidSchemaPlace>) * ;
	schema:geoTouches (@<#ValidSchemaGeospatialGeometry> OR @<#ValidSchemaPlace>) * ;
	schema:geoWithin (@<#ValidSchemaPlace> OR @<#ValidSchemaGeospatialGeometry>) * ;
}