<#ValidSchemaOffer> @<#ValidSchemaIntangible> AND EXTRA a {
	a [schema:AggregateOffer schema:Offer] * ;
	schema:acceptedPaymentMethod (@<#ValidSchemaLoanOrCredit> OR @<#ValidSchemaPaymentMethod>) * ;
	schema:addOn @<#ValidSchemaOffer> * ;
	schema:advanceBookingRequirement @<#ValidSchemaQuantitativeValue> * ;
	schema:aggregateRating @<#ValidSchemaAggregateRating> * ;
	schema:areaServed (@<#ValidSchemaAdministrativeArea> OR @<#ValidSchemaText> OR @<#ValidSchemaPlace> OR @<#ValidSchemaGeoShape>) * ;
	schema:availability @<#ValidSchemaItemAvailability> * ;
	schema:availabilityEnds (@<#ValidSchemaDateTime> OR @<#ValidSchemaTime> OR @<#ValidSchemaDate>) * ;
	schema:availabilityStarts (@<#ValidSchemaDate> OR @<#ValidSchemaTime> OR @<#ValidSchemaDateTime>) * ;
	schema:availableAtOrFrom @<#ValidSchemaPlace> * ;
	schema:availableDeliveryMethod @<#ValidSchemaDeliveryMethod> * ;
	schema:businessFunction @<#ValidSchemaBusinessFunction> * ;
	schema:category (@<#ValidSchemaThing> OR @<#ValidSchemaURL> OR @<#ValidSchemaPhysicalActivityCategory> OR @<#ValidSchemaText>) * ;
	schema:deliveryLeadTime @<#ValidSchemaQuantitativeValue> * ;
	schema:eligibleCustomerType @<#ValidSchemaBusinessEntityType> * ;
	schema:eligibleDuration @<#ValidSchemaQuantitativeValue> * ;
	schema:eligibleQuantity @<#ValidSchemaQuantitativeValue> * ;
	schema:eligibleRegion (@<#ValidSchemaText> OR @<#ValidSchemaPlace> OR @<#ValidSchemaGeoShape>) * ;
	schema:eligibleTransactionVolume @<#ValidSchemaPriceSpecification> * ;
	schema:gtin12 @<#ValidSchemaText> * ;
	schema:gtin13 @<#ValidSchemaText> * ;
	schema:gtin14 @<#ValidSchemaText> * ;
	schema:gtin8 @<#ValidSchemaText> * ;
	schema:includesObject @<#ValidSchemaTypeAndQuantityNode> * ;
	schema:inventoryLevel @<#ValidSchemaQuantitativeValue> * ;
	schema:itemCondition @<#ValidSchemaOfferItemCondition> * ;
	schema:itemOffered (@<#ValidSchemaAggregateOffer> OR @<#ValidSchemaProduct> OR @<#ValidSchemaCreativeWork> OR @<#ValidSchemaService> OR @<#ValidSchemaMenuItem> OR @<#ValidSchemaTrip> OR @<#ValidSchemaEvent>) * ;
	schema:mpn @<#ValidSchemaText> * ;
	schema:offeredBy (@<#ValidSchemaOrganization> OR @<#ValidSchemaPerson>) * ;
	schema:price (@<#ValidSchemaText> OR @<#ValidSchemaNumber>) * ;
	schema:priceCurrency @<#ValidSchemaText> * ;
	schema:priceSpecification @<#ValidSchemaPriceSpecification> * ;
	schema:priceValidUntil @<#ValidSchemaDate> * ;
	schema:review @<#ValidSchemaReview> * ;
	schema:reviews @<#ValidSchemaReview> * ;
	schema:seller (@<#ValidSchemaOrganization> OR @<#ValidSchemaPerson>) * ;
	schema:serialNumber @<#ValidSchemaText> * ;
	schema:sku @<#ValidSchemaText> * ;
	schema:validFrom (@<#ValidSchemaDate> OR @<#ValidSchemaDateTime>) * ;
	schema:validThrough (@<#ValidSchemaDate> OR @<#ValidSchemaDateTime>) * ;
	schema:warranty @<#ValidSchemaWarrantyPromise> * ;
}