<#ValidSchemaLodgingBusiness> @<#ValidSchemaLocalBusiness> AND EXTRA a {
	a [schema:Hostel schema:Motel schema:Resort schema:BedAndBreakfast schema:Hotel schema:Campground schema:LodgingBusiness] * ;
	schema:amenityFeature @<#ValidSchemaLocationFeatureSpecification> * ;
	schema:audience @<#ValidSchemaAudience> * ;
	schema:availableLanguage (@<#ValidSchemaText> OR @<#ValidSchemaLanguage>) * ;
	schema:checkinTime (@<#ValidSchemaDateTime> OR @<#ValidSchemaTime>) * ;
	schema:checkoutTime (@<#ValidSchemaDateTime> OR @<#ValidSchemaTime>) * ;
	schema:numberOfRooms (@<#ValidSchemaQuantitativeValue> OR @<#ValidSchemaNumber>) * ;
	schema:petsAllowed (@<#ValidSchemaText> OR @<#ValidSchemaBoolean>) * ;
	schema:starRating @<#ValidSchemaRating> * ;
}