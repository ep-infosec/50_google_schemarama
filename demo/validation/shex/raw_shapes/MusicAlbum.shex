<#ValidSchemaMusicAlbum> @<#ValidSchemaMusicPlaylist> AND EXTRA a {
	a [schema:MusicAlbum] * ;
	schema:albumProductionType @<#ValidSchemaMusicAlbumProductionType> * ;
	schema:albumRelease @<#ValidSchemaMusicRelease> * ;
	schema:albumReleaseType @<#ValidSchemaMusicAlbumReleaseType> * ;
	schema:byArtist (@<#ValidSchemaMusicGroup> OR @<#ValidSchemaPerson>) * ;
}