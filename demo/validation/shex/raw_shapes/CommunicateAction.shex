<#ValidSchemaCommunicateAction> @<#ValidSchemaInteractAction> AND EXTRA a {
	a [schema:CheckInAction schema:InformAction schema:RsvpAction schema:ShareAction schema:ReplyAction schema:InviteAction schema:CheckOutAction schema:CommentAction schema:AskAction schema:ConfirmAction schema:CommunicateAction] * ;
	schema:about @<#ValidSchemaThing> * ;
	schema:inLanguage (@<#ValidSchemaText> OR @<#ValidSchemaLanguage>) * ;
	schema:language @<#ValidSchemaLanguage> * ;
	schema:recipient (@<#ValidSchemaOrganization> OR @<#ValidSchemaAudience> OR @<#ValidSchemaContactPoint> OR @<#ValidSchemaPerson>) * ;
}