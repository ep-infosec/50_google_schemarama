<#ValidSchemaFinancialProduct> @<#ValidSchemaService> AND EXTRA a {
	a [schema:CurrencyConversionService schema:PaymentService schema:LoanOrCredit schema:DepositAccount schema:PaymentCard schema:BankAccount schema:InvestmentOrDeposit schema:CreditCard schema:FinancialProduct] * ;
	schema:annualPercentageRate (@<#ValidSchemaNumber> OR @<#ValidSchemaQuantitativeValue>) * ;
	schema:feesAndCommissionsSpecification (@<#ValidSchemaText> OR @<#ValidSchemaURL>) * ;
	schema:interestRate (@<#ValidSchemaQuantitativeValue> OR @<#ValidSchemaNumber>) * ;
}