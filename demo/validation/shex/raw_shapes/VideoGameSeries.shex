<#ValidSchemaVideoGameSeries> @<#ValidSchemaCreativeWorkSeries> AND EXTRA a {
	a [schema:VideoGameSeries] * ;
	schema:actor @<#ValidSchemaPerson> * ;
	schema:actors @<#ValidSchemaPerson> * ;
	schema:characterAttribute @<#ValidSchemaThing> * ;
	schema:cheatCode @<#ValidSchemaCreativeWork> * ;
	schema:containsSeason @<#ValidSchemaCreativeWorkSeason> * ;
	schema:director @<#ValidSchemaPerson> * ;
	schema:directors @<#ValidSchemaPerson> * ;
	schema:episode @<#ValidSchemaEpisode> * ;
	schema:episodes @<#ValidSchemaEpisode> * ;
	schema:gameItem @<#ValidSchemaThing> * ;
	schema:gameLocation (@<#ValidSchemaPostalAddress> OR @<#ValidSchemaPlace> OR @<#ValidSchemaURL>) * ;
	schema:gamePlatform (@<#ValidSchemaText> OR @<#ValidSchemaThing> OR @<#ValidSchemaURL>) * ;
	schema:musicBy (@<#ValidSchemaPerson> OR @<#ValidSchemaMusicGroup>) * ;
	schema:numberOfEpisodes @<#ValidSchemaInteger> * ;
	schema:numberOfPlayers @<#ValidSchemaQuantitativeValue> * ;
	schema:numberOfSeasons @<#ValidSchemaInteger> * ;
	schema:playMode @<#ValidSchemaGamePlayMode> * ;
	schema:productionCompany @<#ValidSchemaOrganization> * ;
	schema:quest @<#ValidSchemaThing> * ;
	schema:season (@<#ValidSchemaCreativeWorkSeason> OR @<#ValidSchemaURL>) * ;
	schema:seasons @<#ValidSchemaCreativeWorkSeason> * ;
	schema:trailer @<#ValidSchemaVideoObject> * ;
}