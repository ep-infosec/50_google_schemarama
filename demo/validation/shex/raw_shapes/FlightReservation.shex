<#ValidSchemaFlightReservation> @<#ValidSchemaReservation> AND EXTRA a {
	a [schema:FlightReservation] * ;
	schema:boardingGroup @<#ValidSchemaText> * ;
	schema:passengerPriorityStatus (@<#ValidSchemaQualitativeValue> OR @<#ValidSchemaText>) * ;
	schema:passengerSequenceNumber @<#ValidSchemaText> * ;
	schema:securityScreening @<#ValidSchemaText> * ;
}