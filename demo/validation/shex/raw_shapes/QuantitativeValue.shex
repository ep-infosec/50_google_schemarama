<#ValidSchemaQuantitativeValue> @<#ValidSchemaStructuredValue> AND EXTRA a {
	a [schema:QuantitativeValue] * ;
	schema:additionalProperty @<#ValidSchemaPropertyValue> * ;
	schema:maxValue @<#ValidSchemaNumber> * ;
	schema:minValue @<#ValidSchemaNumber> * ;
	schema:unitCode (@<#ValidSchemaURL> OR @<#ValidSchemaText>) * ;
	schema:unitText @<#ValidSchemaText> * ;
	schema:value (@<#ValidSchemaBoolean> OR @<#ValidSchemaStructuredValue> OR @<#ValidSchemaNumber> OR @<#ValidSchemaText>) * ;
	schema:valueReference (@<#ValidSchemaPropertyValue> OR @<#ValidSchemaStructuredValue> OR @<#ValidSchemaQualitativeValue> OR @<#ValidSchemaQuantitativeValue> OR @<#ValidSchemaEnumeration>) * ;
}