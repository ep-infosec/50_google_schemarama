	schema:publishingPrinciples (@<#ValidSchemaCreativeWork> OR @<#ValidSchemaURL>) * ;
	schema:relatedTo @<#ValidSchemaPerson> * ;
	schema:seeks @<#ValidSchemaDemand> * ;
	schema:sibling @<#ValidSchemaPerson> * ;
	schema:siblings @<#ValidSchemaPerson> * ;
	schema:sponsor (@<#ValidSchemaOrganization> OR @<#ValidSchemaPerson>) * ;
	schema:spouse @<#ValidSchemaPerson> * ;
	schema:taxID @<#ValidSchemaText> * ;
	schema:telephone @<#ValidSchemaText> * ;
	schema:vatID @<#ValidSchemaText> * ;
	schema:weight @<#ValidSchemaQuantitativeValue> * ;
	schema:workLocation (@<#ValidSchemaContactPoint> OR @<#ValidSchemaPlace>) * ;
