	schema:colleagues @<#ValidSchemaPerson> * ;
	schema:contactPoint @<#ValidSchemaContactPoint> * ;
	schema:contactPoints @<#ValidSchemaContactPoint> * ;
	schema:deathDate @<#ValidSchemaDate> * ;
	schema:deathPlace @<#ValidSchemaPlace> * ;
	schema:duns @<#ValidSchemaText> * ;
	schema:email @<#ValidSchemaText> * ;
	schema:familyName @<#ValidSchemaText> * ;
	schema:faxNumber @<#ValidSchemaText> * ;
	schema:follows @<#ValidSchemaPerson> * ;
	schema:funder (@<#ValidSchemaOrganization> OR @<#ValidSchemaPerson>) * ;
	schema:givenName @<#ValidSchemaText> * ;
	schema:globalLocationNumber @<#ValidSchemaText> * ;
	schema:hasOccupation @<#ValidSchemaOccupation> * ;
	schema:hasOfferCatalog @<#ValidSchemaOfferCatalog> * ;
	schema:hasPOS @<#ValidSchemaPlace> * ;
	schema:height (@<#ValidSchemaQuantitativeValue> OR @<#ValidSchemaDistance>) * ;
