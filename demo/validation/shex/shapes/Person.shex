	schema:honorificPrefix @<#ValidSchemaText> * ;
	schema:honorificSuffix @<#ValidSchemaText> * ;
	schema:interactionStatistic @<#ValidSchemaInteractionCounter> * ;
	schema:isicV4 @<#ValidSchemaText> * ;
	schema:knows @<#ValidSchemaPerson> * ;
	schema:makesOffer @<#ValidSchemaOffer> * ;
	schema:memberOf (@<#ValidSchemaOrganization> OR @<#ValidSchemaProgramMembership>) * ;
